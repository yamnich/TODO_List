<%= form_for([@user,@project, @list]) do |f| %>
    <%= render 'shared/error_messages', :object => f.object %>
   <div class="field">
    <%= f.label :name %> <br />
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :description %> <br />
    <%= f.text_area :description %>
  </div>

  <div class="actions">

    <%= f.submit @button_name %>
  </div>
<% end %>